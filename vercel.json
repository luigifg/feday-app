{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*).gz",
        "headers": {
          "Content-Encoding": "gzip",
          "Content-Type": "application/javascript"
        },
        "dest": "/assets/$1.gz"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  